{
  "name": "strawberrywallpaper",
  "version": "v1.3.1",
  "author": "taoacat，流浪记",
  "description": "a wallpaper app by electron",
  "license": "",
  "main": "./dist/electron/main.js",
  "bin": "cli.js",
  "scripts": {
    "build-r": "node .electron-vue/build.js",
    "build-win": "electron-builder --win",
    "build-mac": "electron-builder --mac",
    "build": "npm run build-r && npm run build-win && npm run build-mac",
    "build:dir": "cross-env node .electron-vue/build.js && electron-builder --dir",
    "clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "docs": "docsify serve docs"
  },
  "build": {
    "productName": "Strawberry Wallpaper",
    "appId": "9Y7KR6Q842.com.taoacat.wallpaperapplications",
    "directories": {
      "output": "build"
    },
    "publish": {
      "provider": "generic",
      "url": "https://swallpaper.oss-cn-beijing.aliyuncs.com/version/"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "devDependencies": {
    "ajv": "^6.10.2",
    "babel-eslint": "^10.0.3",
    "cfonts": "^2.4.5",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "drop-console-webpack-plugin": "^3.0.2",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.6",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "electron-fetch": "^1.3.0",
    "electron-log": "^3.0.8",
    "electron-updater": "^4.1.2",
    "element-ui": "^2.12.0",
    "macaddress": "^0.2.9",
    "node-os-utils": "^1.0.7",
    "nodemailer": "^6.3.1",
    "progress": "^2.0.3",
    "qs": "^6.9.0",
    "query-string": "^6.8.3",
    "request": "^2.88.0",
    "style-loader": "^1.0.0",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1",
    "vuex-electron": "^1.0.3",
    "wallpaper": "^4.4.1",
    "wangeditor": "^3.1.1",
    "webp-converter": "^2.2.3"
  }
}
